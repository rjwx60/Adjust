{
  "manifest_version": 2,
  "name": "Adjuster",
  "version": "0.0.1",
  "description": "__MSG_pluginDesc__",
  "icons": 
  {
		"16": "img/icon.png",
		"48": "img/icon.png",
		"128": "img/icon.png"
  },
  "background": {
		"page": "background.html"
  },
  "browser_action": 
	{
		"default_icon": "img/icon.png",
		"default_title": "Adjuster",
		"default_popup": "popup.html"
  },
  "content_scripts": 
	[
		{
			"matches": ["<all_urls>"],
			"js": ["js/content-script.js", "js/vue.min.js", "js/vue_iview.min.js"],
			"run_at": "document_start"
		}
  ],
  "permissions":
	[
		"activeTab",
    "notifications", 
    "webRequest", 
    "webRequestBlocking", 
		"storage",
		"tabs"
	],
	"options_page": "options.html",
	"options_ui":
	{
		"page": "options.html",
		"chrome_style": true
	},
	"homepage_url": "https://www.baidu.com",
  "default_locale": "zh_CN"
}